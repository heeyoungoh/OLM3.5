package xbolt.cmm.service.dtoMdl;

public class RectForOcc {
	private int		m_positionX;
	private int		m_positionY;
	private int		m_width;
	private int		m_height;
	
	// constructors
	public RectForOcc()
	{
		m_positionX = -1;
		m_positionY = -1;
		m_width		= -1;
		m_height	= -1;
	}
	
	public RectForOcc(int positionX, int positionY, int width, int height) 
	{
		m_positionX = positionX;
		m_positionY = positionY;
		m_width		= width;
		m_height	= height;
	}
	
	// get functions
	public int getPositionX()
	{
		return m_positionX;
	}
	
	public int getPositionY()
	{
		return m_positionY;
	}
	
	public int getWidth()
	{
		return m_width;
	}
	
	public int getHeight()
	{
		return m_height;
	}
	
	// set functions
	public void setPositionX(int positionX)
	{
		m_positionX = positionX;
	}
	
	public void setPositionY(int positionY)
	{
		m_positionY = positionY;
	}
	
	public void setWidth(int width)
	{
		m_width = width;
	}
	
	public void setHeight(int height)
	{
		m_height = height;
	}
}

	<select id="getElementClassCode_commonSelect" resultType="java.util.HashMap">
		Select Distinct TI.ClassCode AS CODE, TD.Name AS NAME, ISNULL(TIT.Icon, 'img_job.png') ICON
		From XBOLTADM.TB_ELEMENT TE
		Left Outer Join XBOLTADM.TB_ITEM TI ON TI.ItemID = TE.ObjectID 
		Left Outer Join XBOLTADM.TB_DICTIONARY TD ON TD.TypeCode = TI.ClassCode And TD.Category = 'CLS' ANd TD.LanguageID = 1042
		Left Outer JOIN XBOLTADM.TB_ITEM_TYPE TIT ON TI.ItemTypeCode = TIT.ItemTypeCode
		Where TE.ClassCode IS Not Null
		AND TE.CategoryCode IN ('OJ','MOJ')
		And TE.ModelID = #{modelID}		
		Order By TI.ClassCode ASC
	</select>
	
	<select id="getGroupElementCode_commonSelect" resultType="java.util.HashMap">
		Select TE.ElementID AS CODE
				, TIA.PlainText AS NAME
				, ISNULL(TIT.Icon, 'img_job.png') ICON
				, TI.ItemID
		From XBOLTADM.TB_ELEMENT TE
		Left Outer Join XBOLTADM.TB_Item TI ON TI.ItemID = TE.ObjectID 
		Left Outer Join XBOLTADM.TB_Item_Attr TIA ON TIA.ItemID = TE.ObjectID And TIA.AttrTypeCode  = 'AT00001' And TIA.LanguageID = #{languageID}
		Left Outer JOIN XBOLTADM.TB_ITEM_TYPE TIT ON TI.ItemTypeCode = TIT.ItemTypeCode
		Where TE.ModelID = #{modelID}
		And TI.ClassCode = #{classCode}
		And TE.ElementID IN (Select DisTinct Parent From XBOLTADM.TB_ELEMENT Where ModelId = #{modelID} And Parent != ${modelID}+1)
		Order By TIA.PlainText ASC
	</select>
	
	<select id="getGroupElementClassCode_commonSelect" resultType="java.util.HashMap">
		Select Distinct TI.ClassCode AS CODE, TD.Name AS NAME
		From XBOLTADM.TB_ELEMENT TE
		Left Outer Join XBOLTADM.TB_ITEM TI ON TI.ItemID = TE.ObjectID 
		Left Outer Join XBOLTADM.TB_DICTIONARY TD ON TD.TypeCode = TI.ClassCode And TD.Category = 'CLS' ANd TD.LanguageID = #{languageID}
		Where TE.ClassCode IS Not Null
		AND TE.CategoryCode IN ('OJ','MOJ')
		And TE.ModelID = #{modelID}
		And TE.ElementID IN (Select DisTinct Parent From XBOLTADM.TB_ELEMENT Where ModelId = #{modelID} And Parent != ${modelID}+1 and CategoryCode IN ('OJ','MOJ') )
		Order By TD.Name ASC

					SELECT GroupID
					FROM XBOLTADM.TB_MEMBER_GR
					 WHERE MemberID = #{sessionUserId} 
				)
			)
				
	</select>


	<select id="companyTreeList_treeList" resultType="java.util.HashMap">
		WITH C AS (
		  SELECT DISTINCT COMPANY_ID , PRE_COMPANY_ID , COMPANY_NM FROM( 
		    SELECT 
		        COMPANY_ID , PRE_COMPANY_ID , COMPANY_NM
		      FROM TB_COMPANY C
		     <where>
		     	<choose>
		     		<when test='"1".equals(P_COMPANY_TYP_CD)'>
		     			AND COMPANY_TYP_CD IN ('001')
		     		</when>
		     		<when test='"2".equals(P_COMPANY_TYP_CD)'>
		     			AND COMPANY_TYP_CD IN ('002')
		     		</when>
		     		<when test='"3".equals(P_COMPANY_TYP_CD)'>
		     			AND COMPANY_TYP_CD IN ('003')
		     		</when>
		     		<when test='"23".equals(P_COMPANY_TYP_CD)'>
		     			AND COMPANY_TYP_CD IN ('002','003')
		     		</when>
		     	</choose>
		     </where>
			<if test="COMPANY_ID != null and COMPANY_ID != ''">
				START WITH C.COMPANY_ID = #{COMPANY_ID}
			    CONNECT BY PRIOR  C.COMPANY_ID = C.PRE_COMPANY_ID 
			</if>	  
		  )
		)
		
		
		SELECT 
			  TO_NUMBER(SUBSTR(    COMPANY_ID,3,10))     AS TREE_ID
		    , TO_NUMBER(SUBSTR(PRE_COMPANY_ID,3,10))     AS PRE_TREE_ID
		    , COMPANY_NM                                 AS TREE_NM
		FROM  C
			<where>
				<if test="COMPANY_NM != null and COMPANY_NM != ''">
					COMPANY_NM like '%'||#{COMPANY_NM}||'%'
				</if>	
			</where>
	</select>

	<select id="templType_commonSelect" resultType="java.util.HashMap">
		SELECT tmpl.TemplCode AS CODE, ISNULL(dic.Name, tmpl.TemplCode) NAME
		FROM XBOLTADM.TB_TEMPL tmpl
			LEFT OUTER JOIN XBOLTADM.TB_DICTIONARY dic ON tmpl.TemplCode = dic.TypeCode
			<choose>				
				<when test="sessionCurrLangType != null and sessionCurrLangType != ''">
					AND dic.LanguageID = #{sessionCurrLangType}
				</when>
				<otherwise>
					AND dic.LanguageID = '1042'
				</otherwise>
			</choose>
			<if test='!"1".equals(sessionAuthLev)'>
				INNER JOIN (SELECT TemplCode FROM XBOLTADM.TB_ACCESS_RIGHT WHERE MemberID =  #{sessionUserId} or MemberID in (SELECT GroupID FROM XBOLTADM.TB_MEMBER_GR WHERE MemberID = #{sessionUserId} ) GROUP BY TemplCode) acc ON tmpl.TemplCode = acc.TemplCode
			</if>	
		WHERE ISNULL(tmpl.Deactivated, 0) != 1
		ORDER BY tmpl.SortNum	
	</select>	
	<select id="templCode_commonSelect" resultType="java.util.HashMap">

		And Category = #{category}
	</select>
	
	 <select id="getItemTypeCodeFromCXN_commonSelect" resultType="java.util.HashMap">
	 	With ItemTypeTable ( ItemTypeCode ) 
		AS (
			Select FromItemTypeCode AS ItemTypeCode 
			From XBOLTADM.TB_ITEM_TYPE 
			Where ToItemTypeCode = #{itemTypeCode}
			And ISNULL(Deactivated, 0) != 1 AND Category = 'CN'
			<if test="cxnTypeCodes != null and cxnTypeCodes != ''">
				And ItemTypeCode IN (${cxnTypeCodes})
			</if>
			
			Union All
			
			Select ToItemTypeCode AS ItemTypeCode 
			From XBOLTADM.TB_ITEM_TYPE 
			Where FromItemTypeCode = #{itemTypeCode} 
			And ISNULL(Deactivated, 0) != 1 AND Category = 'CN'
			<if test="cxnTypeCodes != null and cxnTypeCodes != ''">
				And ItemTypeCode IN (${cxnTypeCodes})
			</if>
		)
		Select Distinct T1.ItemTypeCode AS CODE, T2.Name As NAME From ItemTypeTable T1
		Left Outer Join XBOLTADM.TB_DICTIONARY T2 ON T2.TypeCode = T1.ItemTypeCode And T2.LanguageID = #{languageID}
		Inner Join XBOLTADM.TB_Item_Type TIT ON TIT.ItemTypeCode = T1.ItemTypeCode And ISNULL(TIT.Deactivated, 0) != 1
  		
	 </select>
	 
	 <!-- 언어에 따른 Label  조회   -->
	<select id="CSRSTS_commonSelect" resultType="java.util.HashMap">
		SELECT TypeCode AS CODE, ISNULL(Name, '') AS NAME
		FROM XBOLTADM.TB_DICTIONARY
		where 
			LanguageID = #{sessionCurrLangType} 
			<choose>
				<when test="Category != null and Category != ''">
					AND Category= #{Category}
				</when>
				<otherwise>
					AND Category= 'LN'
				</otherwise>
			</choose>
			<if test="TypeCode != null and TypeCode != ''">
				AND TypeCode= #{TypeCode}
			</if>		
			<if test="notTypeCode != null and notTypeCode != ''">
				AND TypeCode NOT IN (${notTypeCode})
			</if>				
			<if test="typeCodeList != null and typeCodeList != ''">
				AND TypeCode IN (${typeCodeList})
			</if>		
	</select>	
	
	<select id="getProcessList_commonSelect" resultType="java.util.HashMap">
		SELECT distinct TIA.ItemID AS CODE, TIA2.PlainText AS NAME, ITM.Identifier	
		FROM XBOLTADM.TB_ITEM_ATTR TIA
			Left Outer JOIN XBOLTADM.TB_ITEM_ATTR TIA2 ON TIA.ItemID = TIA2.ItemID AND TIA2.AttrTypeCode  = 'AT00001'AND TIA2.LanguageID = #{languageID}
			Left Outer JOIN XBOLTADM.TB_ITEM ITM ON TIA.ItemID = ITM.ItemID 
		Where TIA.AttrTypeCode = 'AT00036'  
		And TIA.PlainText = #{procType}
		And ITM.ClassCode = 'CL01009'
		Order by NAME
	</select>
	
	<select id="getItemCntByClass" resultType="java.util.HashMap">
		SELECT
			TI.ClassCode,
			count(TI.ItemID) AS Cnt,
			TD1.Name AS ClassName,
			TD2.Name AS ItemTypeName,
			ISNULL(TIT.ICON,'blank.gif') AS ICON,
			TA.ArcCode,
			TA.Style AS ArcStyle,
			TA.Icon AS ArcIcon,
			TM.URL,
			TM.VarFilter AS MenuVar,
			TA.VarFilter AS ArcVar,
			ISNULL(TIT.DefColor,'') AS DefColor
		FROM XBOLTADM.TB_ITEM TI
		Left Outer Join XBOLTADM.TB_ITEM ST ON TI.ItemID = ST.ToItemID And ST.CategoryCode='ST1'
		Left Outer Join XBOLTADM.TB_ITEM_TYPE TIT ON TIT.ItemTypeCode = TI.ItemTypeCode
		Left Outer Join XBOLTADM.TB_ARC TA ON TA.ArcCode = TIT.DefArc
		Left Outer Join XBOLTADM.TB_MENU TM ON TA.MenuID = TM.MenuID
		Left Outer Join XBOLTADM.TB_DICTIONARY TD1 ON TD1.TypeCode = TI.ClassCode And TD1.LanguageID = #{languageID}
		Left Outer Join XBOLTADM.TB_DICTIONARY TD2 ON TD2.TypeCode = TIT.ItemTypeCode And TD2.LanguageID = #{languageID}
		Where TI.CategoryCode='OJ' AND TI.Deleted != 1 And ST.ToItemID is not null
		Group By TI.ClassCode, TD1.Name, TD2.Name, TIT.ICON, TA.ArcCode, TA.Style, TA.Icon, TM.URL, TM.VarFilter, TA.VarFilter, TIT.DefColor
		Order By TI.ClassCode
	</select>
	
	<select id="getCSVersion_commonSelect" resultType="java.util.HashMap">
		Select ChangeSetID AS CODE, ISNULL(Version,' - ') + '(Updated on '+ ISNULL(CONVERT(char(10),ISNULL(CompletionDT,CreationTime), 120),'')+')' AS NAME 
		From XBOLTADM.TB_CHANGE_SET
		Where ItemID = #{s_itemID} 
		Order By ChangeSetID Desc
	</select>	

<mapper namespace="common_SQL">
	<!-- 언어종류 코드 조회   -->
	<select id="langType_commonSelect" resultType="java.util.HashMap">
		SELECT LanguageID AS CODE, Name NAME, ISNULL(IsDefault,'') AS IsDefault, UPPER(LanguageCode) LANG_CODE
		FROM XBOLTADM.TB_LANGUAGE
		WHERE ISNULL(Deactivated,'0')='0' 
			<if test="SubLanguage != null and SubLanguage != ''">
				AND IsDefault is Null
			</if>
			<if test="otherLangType != null and otherLangType != ''">
				And LanguageID Not In(#{otherLangType})
			</if>
	</select>	
	<!-- 언어에 따른 Label  조회   -->
	<!-- 언어에 따른 Label  조회   -->
	<select id="label_commonSelect" resultType="java.util.HashMap">
		SELECT TypeCode LABEL_CD, LanguageID LANG_TYPE, ISNULL(Name, '') AS LABEL_NM, ISNULL(Description, '') AS Description
		FROM XBOLTADM.TB_DICTIONARY
		<where>
			<choose>
				<when test="Category != null and Category != ''">
					Category= #{Category}
				</when>
				<otherwise>
					Category= 'LN'
				</otherwise>
			</choose>
			<if test="TypeCode != null and TypeCode != ''">
				AND TypeCode= #{TypeCode}
			</if>			
			<if test="LanguageID != null and LanguageID != ''">
				AND LanguageID= #{LanguageID}
			</if>
		</where>
	</select>	
	<!-- 언어에 따른 법인(회사) 조회 -->
	<select id="getCompany_commonSelect" resultType="java.util.HashMap">
		SELECT 
			RTRIM(TeamID) AS CODE
			, Name AS NAME 
		FROM 
			XBOLTADM.TB_TEAM_TXT
		WHERE 
			1=1
			<if test='!"Y".equals(wfadmin)'>
				AND TeamID In (SELECT TeamID FROM XBOLTADM.TB_TEAM WHERE TeamType = 2)
			</if>
			AND	LanguageID = #{sessionCurrLangType} 
	</select>
	
	<!-- ItemTypeCode SelectBox -->
	<select id="itemTypeCode_commonSelect" resultType="java.util.HashMap">
		SELECT DISTINCT  TIT.ItemTypeCode AS CODE, TD.Name NAME
		FROM XBOLTADM.TB_ITEM_TYPE TIT
			Left Outer Join XBOLTADM.TB_DICTIONARY TD on TIT.ItemTypeCode = TD.TypeCode AND TD.LanguageID = #{sessionCurrLangType}
		
		<where>
			<choose>
				<when test="category != null and category != '' and category != 'MOJ' ">
					TIT.Category = #{category}
				</when>
				<otherwise>
					TIT.Category IN ('OJ' , 'VAR')
				</otherwise>
			</choose>
			<if test="Deactivated != null and Deactivated != ''">
				AND TIT.Deactivated != #{Deactivated}
			</if>		
			<if test="objectType != null and objectType != ''">
				AND (TIT.FromItemTypeCode = #{objectType} OR TIT.ToItemTypeCode = #{objectType})
			</if>			
			<if test="itemID != null and itemID != ''">
				AND TIT.ItemTypeCode = (Select ItemTypeCode From XBOLTADM.TB_ITEM Where ItemID = #{itemID})
			</if>
		</where>
	 </select>
	 

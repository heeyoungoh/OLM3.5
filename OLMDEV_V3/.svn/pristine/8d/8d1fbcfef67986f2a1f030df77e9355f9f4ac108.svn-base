/* btn_pack*/
.btn_pack,
.btn_pack *{display:inline-block;overflow:visible;position:relative;margin:0;padding:0;border:0;background:url('../images/btn_pack_new.png') repeat-x;font-size:11px;  color:#000;text-decoration:none !important;vertical-align:top;white-space:nowrap}
.btn_pack{margin-right:4px}
.btn_pack *{left:4px;cursor:pointer;_cursor:hand}

.btn_pack.medium,
.btn_pack.medium *{height:28px;}
.btn_pack.medium{background-position:left 0;z-index:0;}
.btn_pack.small,
.btn_pack.small *{height:24px;line-height:20px}
.btn_pack.small{background-position:left -30px;z-index:0;}
.btn_pack.small *{padding:0 7px 0 5px;background-position:right -30px;z-index:1;}
.btn_pack.medium *{padding:0 10px 0 0;background-position:right top;z-index:1;}
.btn_pack *:hover,
.btn_pack *:active,
.btn_pack *:focus{color:#1684B8}
.btn_pack.strong *{font-weight:bold !important}
.btn_pack.icon *{padding-left:22px;font-weight:400;color:#747171;line-height:24px;}
.btn_pack.icon span{width:20px;margin:2px -20px 0 0;padding:0 !important;height: 20px;}


.btn_pack.nobg,
.btn_pack.nobg *{height:18px;line-height:19px;color:white;}
.btn_pack.nobg{background-position:left 20px}
.btn_pack.nobg *{padding:0;background-position:right -84px;}

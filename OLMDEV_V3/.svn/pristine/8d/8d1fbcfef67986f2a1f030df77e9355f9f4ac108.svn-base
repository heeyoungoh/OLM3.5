/*2depth middle menu*/
.subtabs{padding-left:10px;position:relative;height:26px;background:url('../images/subtabs.png') repeat-x 0 100%;font-size:12px;margin-top:10px;margin-bottom:10px}
.subtabs ul, .subtabs ul li{margin:0;padding:0;}
.subtabs ul li, .subtabs ul li a{background:url('../images/subtabs_off.png') no-repeat}
.subtabs ul li{list-style:none}
.subtabs ul li{float:left;margin-right:-1px;line-height:22px}
.subtabs ul li a{display:inline-block;padding:2px 16px 1px;_padding:3px 16px 0;background-position: 100% 0;font-weight:bold;color:#000;text-decoration:none !important}
.subtabs ul li a:hover{color:#000;}
.subtabs ul li.on, .subtabs ul li.on a{background-image:url('../images/subtabs_on.png'); }/*border-right:1px solid #6097e9*/
.subtabs ul li.on a{color:#fff;}/*3376B8*/




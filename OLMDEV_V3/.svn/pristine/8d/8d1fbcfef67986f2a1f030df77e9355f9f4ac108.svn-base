/*slide Menu*/
.first {text-decoration: none;line-height:34px;text-align:center;color:#fff;}
/*.slidemenu1{width: 100%;height:34px;}*/
.slidemenu{width: 100%;height:34px;background:#586074;} /*bshyun*/
.slidemenu ul{margin: 0;padding: 0;list-style-type: none;}
.slidemenu ul li{position: relative;float: left; z-index:9999;}/*display: inline;background:url('../images/bg_li_bar.png') no-repeat 0 5px;}*/
.slidemenu ul li a{display: block;color: white; vertical-align:middle;padding:0 23px 0 10px;text-decoration: none;line-height:34px;font-weight:bold;background:url('../images/bg_li_bar.png') no-repeat 0 5px;background-position:right;}
.slidemenu ul li a{display:block;}
.slidemenu ul li a:hover{background:#1e2f59;font-weight:bold;color:#fff200;}

.slidemenu ul li ul{clear:both;position: absolute;left: 0;display: block; border:2px solid #465866;padding-right:33px}
.slidemenu ul li ul li{display: block;float: none;}
.slidemenu ul li ul li ul{top: 0;width:100%;}
.slidemenu ul li ul li {margin: 0;padding:0;width:100%;}
.slidemenu ul li ul li a {color:#555;background:#fff;width:100%;} 
.slidemenu ul li ul li a:hover{color:#2e2e3f;background:#d7d7e0;font-weight:bold;}
.slidemenu .topMenuBG{background:#1e2f59;font-weight:bold;color:#fff200}


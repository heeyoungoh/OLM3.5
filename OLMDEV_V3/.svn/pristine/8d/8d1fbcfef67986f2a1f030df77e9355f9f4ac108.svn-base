#reportDetailDiv {
	height: calc(100% - 36px);
	overflow: auto;
}

#reportDetailDiv .report {
	flex-wrap: wrap;
	transition: opacity 0.3s ease-in-out 0s;
	padding: 10px;
	width: calc(60% + 30px);
}

#reportDetailDiv .report li {
	width: calc(33.3333% - 10px);
	height: 220px;
	padding: 1px;
	margin: 0 7.5px 15px;
	box-sizing: border-box;
	cursor: pointer;
}

#reportDetailDiv .report li:nth-child(3n-2) {
	margin-left: 0;
}

#reportDetailDiv .report li:nth-child(3n) {
	margin-right: 0;
}

#reportDetailDiv .report .wrapper {
	width: 100%;
	height: 100%;
	cursor: inherit;
	border-radius: 5px;
	padding: 24px;
	background-color: rgb(255, 255, 255);
	display: flex;
	box-sizing: border-box;
	flex-direction: column;
	box-shadow: rgb(0 0 0 / 20%) 0 0 5px 0px;
	position: relative;
}

#reportDetailDiv .report .icon {
    width: 100%;
    display: flex;
    -webkit-box-align: center;
    -webkit-box-pack: start;
    justify-content: space-between;
}

#reportDetailDiv .report .icon span {
	width: 30px;
	height: 30px;
	border-radius: 3px;
}
#reportDetailDiv .report .icon .authority {
	color: #747171;
}

#reportDetailDiv .report .name {
	margin-top: 15px;
	font-size: 16px;
	font-weight: 700;
	letter-spacing: -0.4px;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
	border-bottom: 1px solid #dfdfdf;
	padding-bottom: 10px;
	color: #333;
}

#reportDetailDiv .report .desc {
	line-height: 1.55;
	color: #333;
	margin-top: 10px;
}

#reportDetailDiv .report .exe {
	cursor: pointer;
	color: #4264ED;
	position: absolute;
	bottom: 24px;
}

#reportDetailDiv .report .exe .mdi {
	color: #4264ED;
	margin-right: 5px;
	font-size: 17px;
}

#reportDetailDiv .report li .name {
	background-image: linear-gradient(to right, #4264ED, #4264ED 50%, #333 50%);
	background-size: 200% 100%;
	background-position: -100%;
	display: inline-block;
	position: relative;
	-webkit-background-clip: text;
	-webkit-text-fill-color: transparent;
	transition: all 0.3s ease-in-out;
}

#reportDetailDiv .report li .name:before {
    content: "";
    background: #4264ED;
    display: block;
    position: absolute;
    bottom:0;
    left: 0;
    width: 0;
    height: 1px;
    transition: all 0.3s ease-in-out;
}

#reportDetailDiv .report li:hover .wrapper {
	background : #F7F8F8;
}

#reportDetailDiv .report li:hover .name {
	background-position: -200%;
	color: #4264ED;
}

#reportDetailDiv .report li:hover .name::before {
	width: 100%;
}


/*orange*/
.tbl_blue01, .tbl_blue01 th, .tbl_blue01 td{border:0;}
.tbl_blue01 {width:100%;border-bottom:1px solid #ddd;/*border-left:1px solid #ddd;*/text-align:center}
.tbl_blue01 th.viewtop{padding:5px;border-top:2px solid #E2DFDF;background-color:#f2f2f2;font-weight:bold; }
.tbl_blue01 td.viewtop{padding:5px;border-top:2px solid #E2DFDF;}
.tbl_blue01 th.viewtopis{border-top:0px;}
.tbl_blue01 td.viewtopis{border-top:0px;}
.tbl_blue01 th{padding:5px 0;border-top:1px solid #ddd;background-color:#f2f2f2;font-weight:bold; }
.tbl_blue01 td{padding:5px;border-top:1px solid #ddd;}
.tbl_blue01 td.tit {padding:5px 0 5px 10px;border-top:1px solid #ddd;color:#000;text-align:left;vertical-align:middle;}
.tbl_blue01 td.noline {text-align:left;padding-left:10px;border-top:0px solid #fff;}
.tbl_blue01 td.doc {padding-left:10px;line-height:20px;border-top:1px solid #f2f2f2;color:#000;text-align:left;vertical-align:middle;}
.tbl_blue01 td.viewtopdoc{padding:5px 0;border-top:1px solid #ddd;color:#000;line-height:20px;vertical-align:middle;text-align:left;padding-left:10px;}
.tbl_blue01 td.btnlast {text-align:right;padding-right:20px;border-top:1px solid #ddd;border-bottom:1px solid #fff;}
.tbl_blue01 td.line {text-align:left;padding-left:10px;border-left:1px solid #ddd;}
.tbl_blue01 th.viewline{padding:5px 0;border-top:1px solid #ddd;border-left:1px solid #ddd;background-color:#f2f2f2;color:#000; font-weight:bold}
.tbl_blue01 th.last {border-right:1px solid #ddd;}

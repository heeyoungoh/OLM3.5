html,body{font-size:14px;}
.container{width:100%; padding:0;}

.btn-cnt{position:relative;}
.btn-cnt span{position:absolute; right:0; top:0; transform: translate(50%,-50%); font-size:10px; display:block; padding:2px 3px; background-color:#eac442; border-radius:3rem;}

.btn{display:inline-block; padding:6px 16px; border-radius:4px;}
.btn.btn-success{background-color:#008bd5; color:#fff; border:0;}
.btn.btn-default{background-color:#f9f9f9; color:#333; border:1px solid #ddd;}


header{width:100%; position:relative; }
header * {box-sizing:border-box;}
header > .container{padding:8px 15px; display:flex; }
header > .container .logo{display:block; height:20px; margin-top:8px; margin-right:30px;}
header > .container .logo img{height:100%;}
header > .container .search_form{height:30px; margin-top:4px; height:100%; border-radius:15px; border:1px solid #aeaeae; position:relative; width:280px; height:30px;}
header > .container .search_form input{border:none;background:none;width: calc(100% - 35px);box-sizing: border-box;padding-left: 15px;}
header > .container .search_form .btn-search{position:absolute; right:0; bottom:5px; top:5px; width:40px; border:0; background:none; cursor:pointer;}
header > .container .search_form .btn-search img{max-width:100%; max-height:100%;}
header > .container .h_right{margin-left:auto; display:flex; height:24px; margin:auto 0 auto auto;}
header > .container .h_right li{padding:0 8px}
header > .container .h_right li
header > .container .h_right li::after{clear:both; content:''; display:block;}
header > .container .h_right li ~ li{border-left:1px solid #e3e3e3;}
header > .container .h_right li select{padding-left:19px; height:100%; background-image:url('../images/main/icon_zoom.png'); background-repeat: no-repeat; background-size: 13px auto; background-position: 7px center; border:0;}
header > .container .h_right li button{border:0; background:none;}
header > .container .h_right li .icon_btn{height:100%; width:auto;padding:0 6px;}
header > .container .h_right li button{float:left; display:block;}
header > .container .h_right li button ~ button{margin-left:3px;}
header > .container .h_right li .btn-logout{padding-left:30px; background-size: 16px auto; background-image: url('../images/main/icon_logout.png'); background-repeat: no-repeat; background-position:6px center; font-size:12px; color:#666; height:24px;}
header > .container .h_right li .btn-alert{background-color:#f2f2f2;width:24px;height:24px;padding:0;border-radius:50%;position: relative;float: left;margin-left: 10px;}
header > .container .h_right li .btn-alert:hover{cursor:pointer;}
header > .container .h_right li .btn-alert img{position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); width:70%; }
header nav{background-color:#008bd5; position:relative;}
header nav .nav-first-content{display:flex; height:32px; line-height:32px;}
header nav .nav-first-content li{line-height:32px; color:#fff; font-size:14px; padding:0 10px;}
header nav .nav-first-content li.active,
header nav .nav-first-content li:hover{color:#333; background-color:#f2f2f2; cursor:default;}
header .btn-sitemap{position:absolute; right:0; background:none; border:0; padding-right:30px; padding-left:10px; color:#fff; background-color:#004368;height: 32px;}
header .btn-sitemap b{position:absolute; top:50%; right: calc(30px / 2); transform:translate(50%,-50%);}
header .btn-sitemap b i{width:12px; height:2px; display:block; background-color:#fff;}
header .btn-sitemap b i:nth-child(2){margin:3px 0;}
.nav-content-2depth{position:absolute; top:100%; left:0; width:100%; background-color:#f2f2f2; padding-top:4px; padding-bottom:4px; border-bottom:1px solid #ddd; min-height:31px;}
.nav-content-2depth ul{display:none;padding:0 4px;}
.nav-content-2depth ul::after{content:''; display:block; clear:both;}
.nav-content-2depth li{float:left; padding:4px 14px;  border-radius:3rem; font-size:14px;}
.nav-content-2depth li:hover{background-color:#dddddd; cursor:default;}
.nav-content-2depth li ~ li{margin-left:10px;}

footer{background-color:#777777; color:#fff; line-height:40px; font-size:12px;}
footer .container{display:flex; width:95%; max-width:1920px; min-width:1024px; margin:auto;}
footer .copy_text{margin-left:20px; margin-right:auto;}

.contents{width:100%;height: 100%;margin: auto 0;max-height:770px;}
.contents .container{width:95%; max-width:1920px; min-width:1024px; margin:auto;}


.tile-group{display:flex;}
.tile-group.group-2 li{width:calc((100% - 16px * 1) / 2); height:auto;}
.tile-group.group-4 li{width:calc((100% - 16px * 3) / 4); height:auto;}
.tile-group li~li{margin-left:16px;}

.contents section {margin-top:30px;}
.contents section ~ section{margin-top:16px; margin-bottom:30px;}

.contents .tile-group > li{border:1px solid #ddd;}
.contents .tile-group li dt{background-color:#f2f2f2;text-align:left;padding:15px;font-size: 13px;font-weight: 700;color: #000;}
.contents .tile-group li dd{padding:15px;}
.contents .tile-group li dd .tile-sub-title{font-size:14px; color:#999; line-height:1.4;padding-bottom:10px; border-bottom:1px solid #ddd; margin-bottom:8px;}
.contents .tile-group li dd .btn{width:100%; padding:10px 0; margin-bottom:20px;}
.contents .tile-group li dd .btn-overlay{padding:20px 0;}
.contents .tile-group li dd .btn-overlay::after{clear:both;content:'';display:block;}
.contents .tile-group li dd .btn-cnt{float:left; width:calc((100% - 1px * 2) / 3); background:none; border:0;}
.contents .tile-group li dd .btn-cnt:nth-child(3n+1){clear:both; margin-left:0;}
.contents .tile-group li dd .btn-cnt:nth-child(3)~.btn-cnt{margin-top:50px;}
.contents .tile-group li dd .btn-cnt .btn-img-wrap{height:32px; width:inherit; margin:auto auto 10px; position:relative;}
.contents .tile-group li dd .btn-cnt .btn-img-wrap img{height:100%;}
.contents .tile-group li dd .btn-cnt .btn-img-wrap span{transform:translateX(50%);}

.tbl-cst{width:100%; border-spacing: 0;}
.tbl-cst tr{border-top:1px solid #ddd; border-bottom:1px solid #ddd;}
.tbl-cst thead tr{border-top:2px solid #ddd; border-bottom:2px solid #ddd;}
.tbl-cst th,
.tbl-cst td{padding:4px 6px; white-space:nowrap; color:#666; font-size:12px; min-width:100px;}
.tbl-cst .color-unset{color:unset;}
.tbl-cst tbody tr:nth-child(2n){background-color:#fff;}
.tbl-cst tbody tr:nth-child(2n+1){background-color:#f9f9f9;}
.table-text-center{text-align:center;}

.swap-tab-container .tab-btn-group{margin-bottom:30px; border-bottom:1px solid #ddd;}
.swap-tab-container .tab-btn-group::after{clear:both; content:''; display:block;}
.swap-tab-container .tab-btn-group .tab-btn{min-width:100px; background-color:#ddd; border:1px solid #ddd; padding:10px 24px; border-radius:4px 4px 0 0; border-bottom:0; margin-bottom:-1px; float:left;}
.swap-tab-container .tab-btn-group .tab-btn ~ .tab-btn{margin-left:3px;}
.swap-tab-container .tab-btn-group .tab-btn.active{background-color:#fff; color:#008bd5; font-weight:700;}
.swap-tab-container .tab-swap-contents .tab-content{display:none;}
.swap-tab-container .tab-swap-contents .tab-content.active{display:block; height:215px;}
.swap-tab-container .tab-swap-contents .tab-content .more_btn-cover{text-align:right; margin-top:12px;}
.swap-tab-container .tab-swap-contents .tab-content .more_btn-cover .btn{width:auto; padding:6px 16px;}

.contents_body_inner {min-width: 733px;max-width: 1300px;}
.detail_settings_list .item {display: -webkit-box;display: -webkit-flex;display: -ms-flexbox;display: flex;padding: 15px 0;border-bottom: 1px solid #eee;align-items: center;}
.detail_settings_list .item h4.title {-webkit-box-sizing: border-box;box-sizing: border-box;-webkit-box-flex: 0;-webkit-flex: 0 0 auto;-ms-flex: 0 0 auto;flex: 0 0 auto;display: inline-block;width: 120px;padding-right: 20px;}
.detail_settings_list .item .option_area {-webkit-box-flex: 1;-webkit-flex: 1 1 auto;-ms-flex: 1 1 auto;flex: 1 1 auto;width: 1px;}
.detail_settings_list .item .input_cover {width: 285px;}
.detail_settings_list .item .input_cover.single {width: calc((100% - 183px)/2 + 468px);}
.lw_input {font-size: 13px;color: #222;line-height: 21px;padding: 6px 0 7px 10px;-webkit-box-sizing: border-box;box-sizing: border-box;height: 36px;border: 1px solid #d9d9d9;border-radius: 2px;width: 100%;-webkit-appearance: none;background:#fff;}
.lw_input:hover, .lw_input:focus {border-color: #157efb;outline: none;}
.lw_btn_select {font-size: 13px;display: inline-block;-webkit-box-sizing: border-box;box-sizing: border-box;height: 36px;border-radius: 2px;line-height: 21px;text-decoration: none;vertical-align: middle;padding: 6px 12px 7px;cursor: pointer;min-width: 49px;color: #222;position: relative;width: 100%;padding-right: 28px;text-align: left;min-width: 120px;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;max-width: 100%;word-wrap: normal;border: 1px solid #d9d9d9;background: #fff url(../images/arrow.gif) no-repeat !important;background-position: calc(100% - 10px) 50% !important;}
.lw_btn_select:hover, .lw_btn_select:focus {border-color: #157efb;}
.lw_toggle {position: absolute;clip: rect(0,0,0,0);width: 1px;height: 1px;margin: -1px;overflow: hidden;}
.lw_toggle+label {position: relative;display: inline-block;font-size: 0;color: transparent;vertical-align: top;padding: 8px 1px;cursor: pointer;}
.lw_toggle+label:before {content: "";position: absolute;top: 6px;left: 0;width: 24px;height: 24px;background-color: #fff;-webkit-box-shadow: 0 0 2px 0 rgb(0 0 0 / 75%);box-shadow: 0 0 2px 0 rgb(0 0 0 / 75%);border-radius: 100%;-webkit-transition: -webkit-transform .2s;transition: -webkit-transform .2s;transition: transform .2s;transition: transform .2s,-webkit-transform .2s;}
.lw_toggle:checked+label:before {-webkit-transform: translateX(17px);transform: translateX(17px);}
.lw_toggle+label:after {content: "";display: inline-block;background-color: #ddd;width: 39px;height: 20px;border-radius: 10px;/* vertical-align: top; */-webkit-transition: background-color .2s;transition: background-color .2s;}
.lw_toggle:checked+label:after {background-color: #157efb;}
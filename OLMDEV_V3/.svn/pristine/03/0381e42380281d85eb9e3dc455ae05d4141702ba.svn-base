#pgCheckBoxDiv{
	bottom: 0;
	position: absolute;
	width: 100%;
	height: 6%;
	align-items: center;
	display: flex;
	justify-content: center;
}
#pgCheckBoxDiv input[type="radio"] {
	position: absolute;
	width: 1px;
	height: 1px;
	padding: 0;
	margin: -1px;
	overflow: hidden;
	clip:rect(0,0,0,0);
	border: 0;
} 
#pgCheckBoxDiv input[type="radio"] + label {
	position: relative;
	padding-left: 17px;
	cursor: pointer;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
} 
#pgCheckBoxDiv input[type="radio"] + label:before {
	content: '';
	position: absolute;
	left: 4px;
	width: 10px;
	height: 10px;
	text-align: center;
	background: #ccc;
	border-radius: 100%;
} 
#pgCheckBoxDiv input[type="radio"]:checked + label:before {
	background: #4064ee;
}


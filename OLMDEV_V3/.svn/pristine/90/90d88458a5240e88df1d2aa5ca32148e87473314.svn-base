ul, ol,li{list-style: none}
ol li{list-style:decimal; margin-left:1em;}

table, th, td{border-collapse: collapse; margin: 0; padding: 0}
table caption{display: none;}


label,button   {cursor:pointer;}
option {font-size: 12px;}
fieldset{border:0; margin: 0px; padding: 0px}
legend  {display: none}

img     {font-size: 0px; border: 0px; vertical-align:middle;}

hr      {display: none}
col     {background: none}

a:link      {color: #000; text-decoration:none;}
a:visited   {color: #000; text-decoration:none;}
a:hover     {color: #000; text-decoration:none;}
a:active    {color: #000; text-decoration:none;}

a { selector-dummy:expression(this.hideFocus=true); }
a:active, a:focus { outline:0; }
a{ -moz-outline-style:none; position: relative }

input, select, img, textarea    {vertical-align:middle !important;font-size: 12px;}
 
select   {border:1px solid #c5c5c5; margin:0;color:#484F59;vertical-align:middle;height:22px;padding: 0 10px 0 5px;}
select.sel{border:1px solid #c5c5c5; margin:0 auto; color:#000;width:98%;height:22px}
select .sel01    {border:1px solid #555666; padding:1px 0px 1px 0px; margin:0 auto; color:#000;height:22px}

textarea {border:0px solid #c5c5c5; padding:1px 0px 1px 0px;color:#000;overflow-y:auto;}
textarea.view {color: #000;line-height: 120%;width:95%;}
textarea.infoview {color: #000;line-height: 120%;width:99%;font-size: 13px;}
textarea.edit {border:1px solid #c5c5c5;}

input.stext {border:1px solid #ccc; color:#000;vertical-align:top; width:98%;height:22px;padding:0 5px;}
input.text {border:1px solid #ccc; color:#000;vertical-align:top; width:98%;height:22px;}
input.image {margin:0;padding:0;vertical-align:middle;}
input.tree_search {border:1px solid #dfdfdf; color:#000; vertical-align:top; width:98%;    padding: 5px 30px 6px 10px;
    background: #f2f2f2;
    border: none;
    border-radius: 4px;
    left: 5px;
    position: relative;
    bottom:0px;}

/* default */
a,img,input {selector-dummy: expression(this.hideFocus=true);} 
